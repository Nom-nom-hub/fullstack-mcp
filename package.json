{
  "name": "fullstack-mcp",
  "version": "1.0.0",
  "description": "Advanced MCP (Model Context Protocol) Server",
  "main": "dist/index.js",
  "bin": {
    "qwen-mcp": "./dist/mcp-server.js",
    "qwen-cli": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "mcp": "ts-node src/mcp-server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts",
    "setup-mcp": "node .qwen/setup.js"
  },
  "keywords": [
    "mcp",
    "ai",
    "server",
    "sandbox",
    "development"
  ],
  "author": "Qwen CLI",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "axios": "^1.11.0",
    "eslint": "^9.35.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  }
}
